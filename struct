#include <iostream>
using namespace std;

struct drob{
    int numerator;
    int denominator;
};

int NOD(int a, int b) {
	while (a > 0 && b > 0) {
		if (a > b) {
			a = a % b;
		}
		else {
			b = b % a;
		}
	}
  return a + b;
}

void reduction(int a, int b){
  int c = NOD(a, b);
  a = a / c;
  b = b / c;
  cout << a << endl;
  cout << "--" << endl;
  cout << b;
}

int main()
{
    drob one;
    cin >> one.numerator;
    cout << "--" << endl;
    cin >> one.denominator;
    reduction(one.numerator, one.denominator);
}
